"\t\t\t\t\t\t\t\t\t\t\t\t<li>aggregate_points</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t<li>aggregate_total_points</li>\n"
"\t\t\t\t\t\t\t\t\t\t\t\t<li>aggregate_timespent</li>\n"
